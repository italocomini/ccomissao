<h1>Seguradora</h1>

<% form_for(@seguradora) do |f| %>
    <%= f.error_messages %>

    <ul class="ul-form">
        <li>
            <label class="lbl-form">Seguradora:*</label>
            <%= f.text_field :nome %>
        </li>
        <li>
            <%= f.submit "Salvar" %><%= link_to 'Voltar', seguradoras_path %>
        </li>
    </ul>
<% end %>
