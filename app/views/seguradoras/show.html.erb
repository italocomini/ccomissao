<h1>Seguradora</h1>

<ul class="ul-show">
    <li>Seguradora: <b><%= @seguradora.nome %></b></li>
    <li>
        Última atualização:
        <b>
            <%= @seguradora.updated_at.strftime("%d/%m/%Y") %>
        </b>
    </li>
    <li class="ul-form-btn">
        <%= link_to 'Editar', edit_seguradora_path(@seguradora) %> |
        <%= link_to 'Voltar', seguradoras_path %>
    </li>
</ul>